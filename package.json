        "laravel-mix": "^1.7.2",
        "lodash": "^4.17.4",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@fullcalendar/core": "^4.3.1",
        "@fullcalendar/daygrid": "^4.3.0",
